 "css": ".logo_text{\n    display:flex;\n    justify-content:center;\n    align-items: center;\n\n}\nh1{\n    margin-top:0px !important;\n    margin-bottom:0px !important;\n    line-height:1 !important;\n\n}\n\n.print-format{\n        font-family: monospace !important;\n\n}\n.customer_detail , .item{\n    display:flex;\n    justify-content:space-between;\n}\n.order_detail{\n    \n}\nhr{\n    margin-top:0px !important;\n    margin-bottom:0px !important;\n}",
